{
	"extends": "airbnb",
	"indent": ["error", "tab"],
	"parser": "babel-eslint",
	"plugins": [
		"import",
		"jsx-a11y",
		"react"
	],
	"env": {
		"browser": true,
		"node": true,
	},
	"rules": {
		"indent": 0,
		"no-tabs": 0,
        "linebreak-style": 0,
		"arrow-body-style": 0,
		"no-param-reassign": 0,
		"react/jsx-indent": 0,
		"react/jsx-indent-props": 0,
        "no-mixed-spaces-and-tabs": 0,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    },
}